<h1>Log In</h1>

<%=render partial: "application/errors", locals: {errors: @errors} %><br>
<br>

<%=form_for :user do |f| %>
    <%=f.label "Username"%>
    <%=f.text_field :username%>
    <%=f.label "Password"%>
    <%=f.password_field :password%>
    <%=f.submit "Log In"%>
<%end%>
<br>
<%=link_to "Login with Facebook", '/auth/facebook', method: :post %>

<%binding.pry%>